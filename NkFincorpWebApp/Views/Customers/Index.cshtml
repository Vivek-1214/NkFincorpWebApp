@model List<CustomersRegistrationVM>
<html>
    <head>
        <title></title>
    </head>
    <body>
        <table class="table tbl-primary">
        <thead>
            <tr>
                <th>Email</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>PositionId</th>
                <th>MobileNumber</th>
                <th>AadharCard</th>
                <th>City</th>
                <th>MaritalStatus</th>
            </tr>
        </thead>
          <tbody>
              <tr>
                @foreach (var CustomersRegistrationVM in Model)
                {
                    <td>@CustomersRegistrationVM.Email</td>
                    <td>@CustomersRegistrationVM.FirstName</td>
                    <td>@CustomersRegistrationVM.LastName</td>
                    <td>@CustomersRegistrationVM.PositionId</td>
                    <td>@CustomersRegistrationVM.MobileNumber</td>
                    <td>@CustomersRegistrationVM.AadharCard</td>
                    <td>@CustomersRegistrationVM.City</td>
                    <td>@CustomersRegistrationVM.MaritalStatus</td>
                    <td> <a> Delete</a></td>
                    <td><a> Update</a></td>
                  }
              </tr>
          </tbody>
        </table>

    </body>
</html>